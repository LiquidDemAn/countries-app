{
	"extends": ["react-app", "react-app/jest"],
	"rules": {
		"no-console": "warn",
		"quotes": ["error", "single"],
		"prefer-const": "error",
		"indent": ["warn", "tab"],
		"semi": ["warn", "always"],
		"import/order": [
			"warn",
			{
				"groups": [
					"builtin",
					"external",
					"internal",
					"parent",
					"sibling",
					"index",
					"object",
					"type"
				]
			}
		]
	}
}
